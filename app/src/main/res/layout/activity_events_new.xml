<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.tracked_entity_instances.TrackedEntityInstancesActivity">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">



        <androidx.appcompat.widget.Toolbar
            android:id="@+id/eventsToolbar"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:background="@drawable/group16682"

            app:popupTheme="@style/AppTheme.PopupOverlay">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/an_title"
                android:textColor="@color/title"
                android:id="@+id/eventTitle"
                android:textSize="20sp"
                android:textAlignment="center"
                android:textStyle="bold"
                android:gravity="center"
                >

            </TextView>

        </androidx.appcompat.widget.Toolbar>

    </com.google.android.material.appbar.AppBarLayout>

    <include layout="@layout/content_events" />

</androidx.coordinatorlayout.widget.CoordinatorLayout>